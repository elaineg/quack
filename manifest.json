{
  "manifest_version": 2,
  "name": "Quack",
  "description": "This extension enables users to interact with a page with voice commands",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "Quack",
    "default_icon": "icon-blue.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "storage",
    "management"
  ]
}
